{
  "name": "next-holiday",
  "version": "0.1.0",
  "private": true,
  "description": "Find the date of your next public holiday based on your location",
  "repository": {
    "type": "git",
    "url": "https://github.com/remnantkevin/next-holiday.git"
  },
  "license": "GPL-3.0-or-later",
  "author": "Kevin Elliott",
  "type": "module",
  "scripts": {
    "build": "tsc --project website/tsconfig.json && vite build",
    "dev": "vite",
    "preview": "vite preview",
    "data:generate:all": "tsc --project data-scripts/tsconfig.json && tsx data-scripts/src/au.ts && tsx data-scripts/src/gb.ts && tsx data-scripts/src/ie.ts && tsx data-scripts/src/nz.ts && tsx data-scripts/src/za.ts"
  },
  "dependencies": {
    "preact": "10.13.2"
  },
  "devDependencies": {
    "@netlify/functions": "1.4.0",
    "@preact/preset-vite": "2.5.0",
    "@types/eslint": "8.37.0",
    "@types/jsdom": "21.1.1",
    "@types/node": "18.16.3",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsonc": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-typescript-sort-keys": "2.3.0",
    "jsdom": "22.1.0",
    "jsonc-eslint-parser": "2.2.0",
    "netlify-cli": "14.3.1",
    "prettier": "2.8.8",
    "prettier-plugin-packagejson": "2.4.3",
    "simple-runtypes": "7.1.3",
    "tsx": "3.12.7",
    "typescript": "5.0.4",
    "undici": "5.22.1",
    "vite": "4.3.3"
  }
